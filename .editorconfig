[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
#以下はVimプラグインでは有効にならない
insert_final_newline = true

[*.html]
indent_style = space
indent_size = 2

[*.jsx]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 2

[*.scss]
indent_style = space
indent_size = 2

[.babelrc]
indent_style = space
indent_size = 2

